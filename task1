vector1 = [(1,3,5,7,9)]
Vector1 = []

for values in vector1:
    for value in values:
        square1 = value **2
        Vector1.append(square1)

print('Given values:', vector1)
print('Square of each number in vector1:',Vector1)
print('Square root:', sum(Vector1)**(1/2))
print('-----------------------------------------------------')

